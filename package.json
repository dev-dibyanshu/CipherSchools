    "install-all": "npm run install-client && npm run install-server",
    "deploy": "cd client && npm run deploy"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dev-dibyanshu/CipherSchools.git"
  },
  "keywords": [
    "sql",
    "learning",
    "education",
    "postgresql",
    "react",
    "nodejs"
  ],
