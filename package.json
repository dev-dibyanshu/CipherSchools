{
  "name": "ciphersqlstudio",
  "version": "1.0.0",
  "description": "Interactive SQL Learning Platform with LeetCode-Style Validation",
  "main": "server/app.js",
  "scripts": {
    "start": "cd server && npm start",
    "dev": "cd server && npm run dev",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
